#!/usr/bin/python

import subprocess

rc = subprocess.Popen( 'git log --all --format="%H %ae %ce %s"' , stdout=subprocess.PIPE )
for line in rc.stdout.readlines() :
    print line[:-1]
print

rc = subprocess.Popen( 'git log --all --format="%H %at %ct %P"' , stdout=subprocess.PIPE )
for line in rc.stdout.readlines() :
    print line[:-1]
print

